{
    "name": "jlm/aws-bundle",
    "type": "symfony-bundle",
    "description": "Symfony wrapper for Amazon PHP SDK 2.x",
    "keywords": ["amazon web services","aws"],
    "homepage": "http://github.com/jmcclell/JLMAwsBundle",
    "license": "MIT",
    "authors": [
        {
            "name": "Jason McClellan",
            "email": "jason@jasonmcclellan.net"
        }
    ],
    "require": {
        "symfony/framework-bundle": "~2.1",
        "symfony/process": "~2.1",
        "symfony/finder": "~2.1"
    },
    "require-dev": {
        "symfony/validator": "*",
        "symfony/form": "*",
        "symfony/class-loader": "*",
        "symfony/yaml": "*",
        "symfony/browser-kit": "*"
    },
    "autoload": {
        "psr-0": { "JLM\\AwsBundle": "" }
    },
    "target-dir": "JLM/AwsBundle",
    "extra": {
        "branch-alias": {
            "dev-master": "1.0-dev"
        }
    }
}