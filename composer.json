{
    "name": "jlm/aws-bundle",
    "type": "symfony-bundle",
    "description": "Symfony wrapper for Amazon PHP SDK 2.x",
    "keywords": ["amazon web services","aws"],
    "homepage": "http://github.com/jmcclell/JLMAwsBundle",
    "license": "MIT",
    "authors": [
        {
            "name": "Jason McClellan",
            "email": "jason@jasonmcclellan.net"
        }
    ],
    "require": {
        "symfony/framework-bundle": "~2.1",
        "aws/aws-sdk-php": "~2.5"
    },
    "autoload": {
        "psr-0": { "JLM\\AwsBundle": "" }
    },
    "target-dir": "JLM/AwsBundle",
    "extra": {
        "branch-alias": {
            "dev-1.0.x": "1.0-dev"
        }
    },
    "require-dev": {
        "symfony/console": "~2.1",
        "symfony/finder": "~2.1",
        "symfony/browser-kit": "~2.1",
        "satooshi/php-coveralls": "dev-master"
    },
    "suggest": {
        "playbloom/guzzle-bundle": "For profiling and debug tooling for AWS service clients"
    }
}
