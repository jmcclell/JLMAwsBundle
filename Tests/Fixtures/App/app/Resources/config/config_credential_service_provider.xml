<?xml version="1.0" ?>

<container xmlns="http://symfony.com/schema/dic/services"
           xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
           xmlns:jlm-aws="http://symfony.com/schema/dic/jlm-aws"
>
    <services>
        <service id="jlm_aws.credentials_provider" class="JLM\AwsBundle\Tests\Fixtures\MockService\CredentialsProvider">
        </service>
    </services>
    <jlm-aws:config>
            <jlm-aws:default-settings>
                <jlm-aws:credentials 
                        key="MY_UNUSED_KEY"
                        secret="MY_UNUSED_SECRET"
                        provider_service="@jlm_aws.credentials_provider"
                   />
            </jlm-aws:default-settings>
    </jlm-aws:config>
</container>


